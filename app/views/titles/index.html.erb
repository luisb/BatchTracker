<h1>Listing titles</h1>

<table>
  <tr>
    <th>Lccn</th>
    <th>Title</th>
    <th>City publication</th>
    <th>Beg date</th>
    <th>End date</th>
    <th>Issue edition pattern</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @titles.each do |title| %>
  <tr>
    <td><%= title.lccn %></td>
    <td><%= title.title %></td>
    <td><%= title.city_publication %></td>
    <td><%= title.beg_date %></td>
    <td><%= title.end_date %></td>
    <td><%= title.issue_edition_pattern %></td>
    <td><%= link_to 'Show', title %></td>
    <td><%= link_to 'Edit', edit_title_path(title) %></td>
    <td><%= link_to 'Destroy', title, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Title', new_title_path %>
