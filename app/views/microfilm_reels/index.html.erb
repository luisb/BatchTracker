<h1>Listing microfilm_reels</h1>

<table>
  <tr>
    <th>Date shipped scan</th>
    <th>Tracking number</th>
    <th>Date tiffs shipped ucr</th>
    <th>Date film returned</th>
    <th>Shelved at ucb</th>
    <th>Date shipped rescan</th>
    <th>Rescan tracking number</th>
    <th>Date shipped lc</th>
    <th>Vendor project</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @microfilm_reels.each do |microfilm_reel| %>
  <tr>
    <td><%= microfilm_reel.date_shipped_scan %></td>
    <td><%= microfilm_reel.tracking_number %></td>
    <td><%= microfilm_reel.date_tiffs_shipped_ucr %></td>
    <td><%= microfilm_reel.date_film_returned %></td>
    <td><%= microfilm_reel.shelved_at_ucb %></td>
    <td><%= microfilm_reel.date_shipped_rescan %></td>
    <td><%= microfilm_reel.rescan_tracking_number %></td>
    <td><%= microfilm_reel.date_shipped_lc %></td>
    <td><%= microfilm_reel.vendor_project_id %></td>
    <td><%= link_to 'Show', microfilm_reel %></td>
    <td><%= link_to 'Edit', edit_microfilm_reel_path(microfilm_reel) %></td>
    <td><%= link_to 'Destroy', microfilm_reel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

